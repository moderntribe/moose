	"name": "tribe/announcements",
	"version": "0.1.0",
	"title": "Announcements",
	"category": "theme",
	"icon": "megaphone",
	"description": "A block to display globally configured announcements",
	"supports": {
		"html": false,
		"align": [
			"wide",
			"grid",
