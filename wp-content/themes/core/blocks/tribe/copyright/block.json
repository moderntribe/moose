{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tribe/copyright",
	"version": "1.0.0",
	"title": "Copyright",
	"category": "theme",
	"description": "Displays the copyright information for the site.",
	"icon": "info-outline",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"color": {
			"background": false,
			"text": true
		},
		"typography": {
			"fontSize": true
		}
	},
	"attributes": {
		"showStartingYear": {
			"type": "boolean"
		},
		"startingYear": {
			"type": "string"
		},
		"copyrightText": {
			"type": "string"
		}
	},
	"textdomain": "tribe",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
