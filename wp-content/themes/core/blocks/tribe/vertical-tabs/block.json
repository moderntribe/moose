{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tribe/vertical-tabs",
	"version": "1.0.0",
	"title": "Vertical Tabs",
	"category": "theme",
	"icon": "align-pull-right",
	"description": "Tab content displayed vertically",
	"attributes": {
		"blockUpdated": {
			"type": "string",
			"default": ""
		},
		"currentActiveTabInstanceId": {
			"type": "string",
			"default": ""
		},
		"tabs": {
			"type": "array",
			"default": []
		}
	},
	"supports": {
		"html": false,
		"align": ["wide", "grid", "full"],
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"providesContext": {
		"tribe/vertical-tabs/currentActiveTabInstanceId": "currentActiveTabInstanceId"
	},
	"textdomain": "tribe",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
