{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tribe/announcement-renderer",
	"version": "0.1.0",
	"title": "Announcement Renderer",
	"category": "theme",
	"icon": "block-default",
	"description": "A block to output the announcement block from CPT",
	"supports": {
		"html": false,
		"align": [
			"wide",
			"grid",
			"full"
		],
		"spacing": {
			"margin": true,
			"padding": false
		}
	},
	"attributes": {
		"placement": {
			"type": "string",
			"default": null
		}
	},
	"textdomain": "tribe",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
