{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tribe/horizontal-tab",
	"version": "0.1.0",
	"title": "Horizontal Tab",
	"category": "theme",
	"description": "A child block of the Horizontal Tabs block",
	"icon": "text-page",
	"parent": ["tribe/horizontal-tabs"],
	"supports": {
		"html": false,
		"spacing": {
			"margin": false,
			"padding": true
		}
	},
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"tabLabel": {
			"type": "string",
			"default": ""
		}
	},
	"usesContext": ["tribe/horizontal-tabs/currentActiveTabInstanceId"],
	"textdomain": "tribe",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}
