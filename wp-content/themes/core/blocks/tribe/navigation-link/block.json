{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tribe/navigation-link",
	"version": "0.1.0",
	"title": "Navigation Link",
	"category": "theme",
	"icon": "plus-alt",
	"description": "A block for adding simple menu item link to the mega navigation.",
	"parent": [ "tribe/mega-nav-standard-item" ],
	"attributes": {
		"text": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": ""
		},
		"target": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "tribe",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
